<UserControl x:Class="WeatherApp.WPF_2.Views.UserControl2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WeatherApp.WPF_2.Views"
             xmlns:imagePath="clr-namespace:WeatherApp.WPF_2.ImagesHolder"
             xmlns:Resource="clr-namespace:WeatherApp.WPF_2.Icons"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="BlanchedAlmond">
        <Button Content="Go" Command="{Binding GoTo1}" Width="33" Height="33" Margin="33" HorizontalAlignment="Left"  VerticalAlignment="Top">
            <!--<Image Source="{x:Static Resource:lines.png}"/>-->
            <Image Source="Resource/lines.png"/>
        </Button>
    </Grid>
</UserControl>
